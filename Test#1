-- Script: Banana Cat Hub Free Edition for Blox Fruits
-- Ngày tạo: 27/03/2025
-- Tác giả: Grok (xAI)
-- Miễn phí cho mọi người sử dụng!

-- Dịch vụ và biến toàn cục
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Workspace = game:GetService("Workspace")
local VirtualUser = game:GetService("VirtualUser")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Tạo GUI Key
local KeyGui = Instance.new("ScreenGui")
KeyGui.Name = "KeyGui"
KeyGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local KeyFrame = Instance.new("Frame")
KeyFrame.Size = UDim2.new(0, 300, 0, 200)
KeyFrame.Position = UDim2.new(0.5, -150, 0.5, -100)
KeyFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
KeyFrame.BorderSizePixel = 0
KeyFrame.Parent = KeyGui

local KeyTitle = Instance.new("TextLabel")
KeyTitle.Size = UDim2.new(1, 0, 0, 50)
KeyTitle.Position = UDim2.new(0, 0, 0, 0)
KeyTitle.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
KeyTitle.Text = "Enter Key"
KeyTitle.TextColor3 = Color3.fromRGB(255, 255, 0)
KeyTitle.Font = Enum.Font.SourceSansBold
KeyTitle.TextSize = 24
KeyTitle.Parent = KeyFrame

local KeyInput = Instance.new("TextBox")
KeyInput.Size = UDim2.new(0, 260, 0, 40)
KeyInput.Position = UDim2.new(0, 20, 0, 70)
KeyInput.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
KeyInput.Text = ""
KeyInput.PlaceholderText = "Input Key Here"
KeyInput.TextColor3 = Color3.fromRGB(255, 255, 255)
KeyInput.Font = Enum.Font.SourceSans
KeyInput.TextSize = 18
KeyInput.Parent = KeyFrame

local SubmitButton = Instance.new("TextButton")
SubmitButton.Size = UDim2.new(0, 260, 0, 40)
SubmitButton.Position = UDim2.new(0, 20, 0, 120)
SubmitButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
SubmitButton.Text = "Submit"
SubmitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
SubmitButton.Font = Enum.Font.SourceSans
SubmitButton.TextSize = 18
SubmitButton.Parent = KeyFrame

-- Key chính xác
local CorrectKey = "1"

-- Hàm kiểm tra key và hiển thị GUI chính
local function CheckKey()
    if KeyInput.Text == CorrectKey then
        KeyGui:Destroy()
        -- Tạo GUI chính
        local ScreenGui = Instance.new("ScreenGui")
        ScreenGui.Name = "BananaCatHub"
        ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

        local MainFrame = Instance.new("Frame")
        MainFrame.Size = UDim2.new(0, 400, 0, 600)
        MainFrame.Position = UDim2.new(0.5, -200, 0.5, -300)
        MainFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        MainFrame.BorderSizePixel = 0
        MainFrame.Parent = ScreenGui

        local Title = Instance.new("TextLabel")
        Title.Size = UDim2.new(1, 0, 0, 50)
        Title.Position = UDim2.new(0, 0, 0, 0)
        Title.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        Title.Text = "Banana Cat Hub - Blox Fruits Edition"
        Title.TextColor3 = Color3.fromRGB(255, 255, 0)
        Title.Font = Enum.Font.SourceSansBold
        Title.TextSize = 24
        Title.Parent = MainFrame

        -- Hàm tạo nút
        local function CreateButton(name, position, callback)
            local Button = Instance.new("TextButton")
            Button.Size = UDim2.new(0, 380, 0, 40)
            Button.Position = position
            Button.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
            Button.Text = name
            Button.TextColor3 = Color3.fromRGB(255, 255, 255)
            Button.Font = Enum.Font.SourceSans
            Button.TextSize = 18
            Button.Parent = MainFrame
            Button.MouseButton1Click:Connect(callback)
            return Button
        end

        -- Biến trạng thái
        local AutoFarmEnabled = false
        local AutoQuestEnabled = false
        local KillAuraEnabled = false
        local AutoChestEnabled = false
        local AutoSkillEnabled = false
        local AutoV4Enabled = false
        local AutoV4DracoEnabled = false
        local AutoHuntDragonIslandEnabled = false
        local AutoFarmDragonIslandEnabled = false

        -- Nút Auto Farm Level
        local AutoFarmButton = CreateButton("Auto Farm Level: OFF", UDim2.new(0, 10, 0, 60), function()
            AutoFarmEnabled = not AutoFarmEnabled
            AutoFarmButton.Text = "Auto Farm Level: " .. (AutoFarmEnabled and "ON" or "OFF")
            if AutoFarmEnabled then
                spawn(function()
                    while AutoFarmEnabled do
                        for _, enemy in pairs(Workspace.Enemies:GetChildren()) do
                            if enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
                                LocalPlayer.Character.HumanoidRootPart.CFrame = enemy.HumanoidRootPart.CFrame * CFrame.new(0, 5, -5)
                                VirtualUser:ClickButton1(Vector2.new())
                                wait(0.5)
                            end
                        end
                        wait(0.1)
                    end
                end)
            end
        end)

        -- Nút Auto Quest
        local AutoQuestButton = CreateButton("Auto Quest: OFF", UDim2.new(0, 10, 0, 110), function()
            AutoQuestEnabled = not AutoQuestEnabled
            AutoQuestButton.Text = "Auto Quest: " .. (AutoQuestEnabled and "ON" or "OFF")
            if AutoQuestEnabled then
                spawn(function()
                    while AutoQuestEnabled do
                        local questNPC = Workspace.NPCs:FindFirstChildOfClass("Model")
                        if questNPC and questNPC:FindFirstChild("HumanoidRootPart") then
                            LocalPlayer.Character.HumanoidRootPart.CFrame = questNPC.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
                            ReplicatedStorage.Remotes.CommF_:InvokeServer("StartQuest", questNPC.Name, 1)
                            wait(2)
                        end
                        wait(5)
                    end
                end)
            end
        end)

        -- Nút Kill Aura
        local KillAuraButton = CreateButton("Kill Aura: OFF", UDim2.new(0, 10, 0, 160), function()
            KillAuraEnabled = not KillAuraEnabled
            KillAuraButton.Text = "Kill Aura: " .. (KillAuraEnabled and "ON" or "OFF")
            if KillAuraEnabled then
                spawn(function()
                    while KillAuraEnabled do
                        for _, enemy in pairs(Workspace.Enemies:GetChildren()) do
                            if enemy:FindFirstChild("Humanoid") and (enemy.HumanoidRootPart.Position - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 15 then
                                ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
                                VirtualUser:ClickButton1(Vector2.new())
                            end
                        end
                        wait(0.1)
                    end
                end)
            end
        end)

        -- Nút Auto Chest
        local AutoChestButton = CreateButton("Auto Chest: OFF", UDim2.new(0, 10, 0, 210), function()
            AutoChestEnabled = not AutoChestEnabled
            AutoChestButton.Text = "Auto Chest: " .. (AutoChestEnabled and "ON" or "OFF")
            if AutoChestEnabled then
                spawn(function()
                    while AutoChestEnabled do
                        for制的, chest in pairs(Workspace:GetChildren()) do
                            if chest.Name:find("Chest") then
                                LocalPlayer.Character.HumanoidRootPart.CFrame = chest.CFrame
                                wait(0.5)
                            end
                        end
                        wait(1)
                    end
                end)
            end
        end)

        -- Nút Auto Skills
        local AutoSkillButton = CreateButton("Auto Skills: OFF", UDim2.new(0, 10, 0, 260), function()
            AutoSkillEnabled = not AutoSkillEnabled
            AutoSkillButton.Text = "Auto Skills: " .. (AutoSkillEnabled and "ON" or "OFF")
            if AutoSkillEnabled then
                spawn(function()
                    while AutoSkillEnabled do
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
                        for _, skill in pairs({"Z", "X", "C", "V"}) do
                            game:GetService("VirtualInputManager"):SendKeyEvent(true, skill, false, game)
                            wait(0.2)
                            game:GetService("VirtualInputManager"):SendKeyEvent(false, skill, false, game)
                        end
                        wait(1)
                    end
                end)
            end
        end)

        -- Nút Auto V4
        local AutoV4Button = CreateButton("Auto V4: OFF", UDim2.new(0, 10, 0, 310), function()
            AutoV4Enabled = not AutoV4Enabled
            AutoV4Button.Text = "Auto V4: " .. (AutoV4Enabled and "ON" or "OFF")
            if AutoV4Enabled then
                spawn(function()
                    while AutoV4Enabled do
                        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(28734, 14895, -95)
                        wait(2)
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("AwakeningChanger", "V4")
                        wait(5)
                    end
                end)
            end
        end)

        -- Nút Auto V4 Draco
        local AutoV4DracoButton = CreateButton("Auto V4 Draco: OFF", UDim2.new(0, 10, 0, 360), function()
            AutoV4DracoEnabled = not AutoV4DracoEnabled
            AutoV4DracoButton.Text = "Auto V4 Draco: " .. (AutoV4DracoEnabled and "ON" or "OFF")
            if AutoV4DracoEnabled then
                spawn(function()
                    while AutoV4DracoEnabled do
                        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(28734, 14895, -95)
                        wait(2)
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("AwakeningChanger", "V4")
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("CheckTempleDoor")
                        wait(5)
                    end
                end)
            end
        end)

        -- Nút Auto Hunt Dragon Island
        local AutoHuntDragonIslandButton = CreateButton("Auto Hunt Dragon Island: OFF", UDim2.new(0, 10, 0, 410), function()
            AutoHuntDragonIslandEnabled = not AutoHuntDragonIslandEnabled
            AutoHuntDragonIslandButton.Text = "Auto Hunt Dragon Island: " .. (AutoHuntDragonIslandEnabled and "ON" or "OFF")
            if AutoHuntDragonIslandEnabled then
                spawn(function()
                    while AutoHuntDragonIslandEnabled do
                        local mirage = Workspace:FindFirstChild("Mirage Island")
                        if mirage then
                            LocalPlayer.Character.HumanoidRootPart.CFrame = mirage:FindFirstChild("Root").CFrame * CFrame.new(0, 10, 0)
                        else
                            LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-12500, 300, -7500)
                            wait(5)
                        end
                        wait(2)
                    end
                end)
            end
        end)

        -- Nút Auto Farm Dragon Island
        local AutoFarmDragonIslandButton = CreateButton("Auto Farm Dragon Island: OFF", UDim2.new(0, 10, 0, 460), function()
            AutoFarmDragonIslandEnabled = not AutoFarmDragonIslandEnabled
            AutoFarmDragonIslandButton.Text = "Auto Farm Dragon Island: " .. (AutoFarmDragonIslandEnabled and "ON" or "OFF")
            if AutoFarmDragonIslandEnabled then
                spawn(function()
                    while AutoFarmDragonIslandEnabled do
                        local mirage = Workspace:FindFirstChild("Mirage Island")
                        if mirage then
                            for _, enemy in pairs(mirage:GetDescendants()) do
                                if enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
                                    LocalPlayer.Character.HumanoidRootPart.CFrame = enemy.HumanoidRootPart.CFrame * CFrame.new(0, 5, -5)
                                    ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
                                    VirtualUser:ClickButton1(Vector2.new())
                                    wait(0.5)
                                end
                            end
                        end
                        wait(0.1)
                    end
                end)
            end
        end)

        -- Nút Teleport đến các đảo
        local TeleportButton = CreateButton("Teleport to Island", UDim2.new(0, 10, 0, 510), function()
            local islands = {
                ["Marine Starter"] = CFrame.new(-2574, 6, 2045),
                ["Middle Town"] = CFrame.new(-655, 7, 1576),
                ["Cafe"] = CFrame.new(-379, 73, 293),
                ["Mansion"] = CFrame.new(-390, 331, 685),
                ["Temple of Time"] = CFrame.new(28734, 14895, -95),
                ["Mirage Island"] = CFrame.new(-12500, 300, -7500)
            }
            for name, cframe in pairs(islands) do
                if (LocalPlayer.Character.HumanoidRootPart.Position - cframe.Position).Magnitude > 1000 then
                    LocalPlayer.Character.HumanoidRootPart.CFrame = cframe
                    break
                end
            end
        end)

        -- Nút Speed Hack
        local SpeedButton = CreateButton("Speed Hack (50)", UDim2.new(0, 10, 0, 560), function()
            LocalPlayer.Character.Humanoid.WalkSpeed = 50
        end)

        -- Nút Jump Power
        local JumpButton = CreateButton("Jump Power (100)", UDim2.new(0, 10, 0, 610), function()
            LocalPlayer.Character.Humanoid.JumpPower = 100
        end)

        -- Nút Đóng GUI
        local CloseButton = CreateButton("Close", UDim2.new(0, 10, 0, 660), function()
            ScreenGui:Destroy()
        end)

        -- Làm GUI có thể kéo
        local dragging, dragInput, dragStart, startPos
        MainFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
                dragStart = input.Position
                startPos = MainFrame.Position
            end
        end)

        MainFrame.InputChanged:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement then
                dragInput = input
            end
        end)

        game:GetService("UserInputService").InputChanged:Connect(function(input)
            if input == dragInput and dragging then
                local delta = input.Position - dragStart
                MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
            end
        end)

        MainFrame.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = false
            end
        end)

        print("Banana Cat Hub Free Edition cho Blox Fruits đã được tải!")
    else
        KeyTitle.Text = "Wrong Key! Try Again"
        wait(1)
        KeyTitle.Text = "Enter Key"
    end
end

SubmitButton.MouseButton1Click:Connect(CheckKey)
